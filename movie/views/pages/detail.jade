extends ../layout

block content
	.container
		.row
			.col-md-7
				embed(src="#{movie.flash}", allowFullScreen="true", quality="high", width="720", height="450", align="middle", type="application/x-shockwave-flash")
			.col-md-5
				dl.dl-horizontal
					dt 电影名字
					dd= movie.title
					dt 电影导演
					dd= movie.doctor
					dt 国家
					dd= movie.contry
					dt 语言
					dd= movie.language
					dt 上映年份
					dd= movie.year
					dt 简介
					dd= movie.summary
